		<script>
			function openBtn(me, btn) {
				var id = "#"+me;
				var bouton = document.querySelector(id);
				var status = bouton.dataset.status;
				
				if(status == "close") {
					bouton.dataset.status = 'open';
					var btn_id = '#'+btn;
					document.querySelector(btn_id).innerHTML = "<svg width='20' height='20' viewBox='0 0 16 10'><path fill='none' stroke='currentColor' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.042' d='M15.516 9.153L8.28.976a.325.325 0 00-.487 0L.557 9.153'></path></svg>";
					var answer_id = "#"+bouton.dataset.answer;
					document.querySelector(answer_id).style.display = "block";
				
				} else {
					bouton.dataset.status = 'close';
					var btn_id = '#'+btn;
					document.querySelector(btn_id).innerHTML = "<svg width='20' height='20' viewBox='0 0 16 10'><path fill='none' stroke='currentColor' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.042' d='M.564.861l7.222 8.164a.325.325 0 00.459.027l.028-.027L15.496.86'></path></svg>";
					var answer_id = "#"+bouton.dataset.answer;
					document.querySelector(answer_id).style.display = "none";
				}
			}
		</script>




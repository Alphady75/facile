    <style>
        .phone-icon {
            position: relative;
            height: 65px;
            width: 65px;
            background-color: #fff;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-left: auto;
            margin-right: auto;
        }
        .phone-icon:before {
            position: absolute;
            top: 50%;
            left: -77px;
            width: 57px;
            height: 1px;
            content: "";
            background-color: rgba(var(--oslim-white-rgb), .20);
            transform: translateY(-50%);
            background-color: white;
        }

        .phone-icon:after {
            position: absolute;
            top: 50%;
            right: -77px;
            width: 57px;
            height: 1px;
            content: "";
            background-color: rgba(var(--oslim-white-rgb), .20);
            transform: translateY(-50%);
            background-color: white;
        }

        .form-control {
            padding: 10px 15px;
            border-radius: 4px;
            height: 50px;
            line-height: 50px;
            background-color: #f4f5f8;
            border-color: #f4f5f8;

<!-- START BLOG -->		
<section id="blog" class="fresh-news section-padding">
    <div class="container">
        <div class="section-title text-center wow zoomIn">
			<h2>Actualités</h2>	
        </div>				
        <div class="row">	
            {% for article in articles %}
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 wow fadeInUp" data-wow-duration="1s" data-wow-delay="0.1s" data-wow-offset="0">
                    <div class="single_blog">
                        <div class="blog_img">
                            <a href="{{path('article', {id : article.id, slug : article.slug})}}"><img src="/article/{{article.imageName}}" class="img-fluid" alt="image" /></a>
                        </div>
                        <div class="blog_content">
                            <span class="blog_cat">
                                <a href="javascript:void(0);">JVGO</a>
                            </span>
                            <h3><a href="{{path('article', {id : article.id, slug : article.slug})}}">{{article.titre}}</a></h3>								
                            <div class="blog-meta">
                                <span class="blog-author">
                                    <a href="{{path('article', {id : article.id, slug : article.slug})}}">{{article.description | striptags | slice(0, 25) |raw }}...</a>
                                </span> <br>
                                <span class="blog-date">{{article.createdAt | date('d/m/Y')}}</span>								
                            </div>
                            <a href="{{path('article', {id : article.id, slug : article.slug})}}" class="blog_btn"><i class="ti-arrow-right"></i></a>
                        </div>
                    </div>
                </div><!-- END COL-->	
            {% endfor %}
        </div><!-- END ROW -->
    </div><!-- END CONTAINER -->	
</section>
<!-- END BLOG -->

